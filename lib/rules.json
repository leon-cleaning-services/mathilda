{
  "version": 1,
  "rules": {
    "google_analytics": {
      "type": "remove_params",
      "parameters": [
        "ga_*",
        "utm_*",
        "gclid"
      ]
    },
    "google_play_store": {
      "type": "remove_params",
      "parameters": [
        "hl",
        "selections"
      ],
      "domains": [
        "store.google.com"
      ]
    }
  }
}
